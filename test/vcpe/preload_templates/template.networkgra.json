          "subnet-name": "${network_name}"
        }
      ],
      "physical-network-name": "Not Aplicable",
      "is-provider-network": "FALSE",
      "is-shared-network": "FALSE",
      "is-external-network": "FALSE",
      "vpn-bindings": [],
      "network-policy": [],
      "route-table-reference": []
    }
  }
